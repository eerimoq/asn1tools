-- 1 bit signed
R ::= INTEGER (-1..0)

-- 2 bits signed
S ::= INTEGER (-2..1)

T ::= INTEGER (-1..2)

-- 7 bits signed
U ::= INTEGER (-64..63)

-- 8 bits signed
V ::= INTEGER (-128..127)

-- 9 bits signed
W ::= INTEGER (-1..510)

-- 10 bits signed
X ::= INTEGER (-2..510)

-- 10 bits unsigned
Y ::= INTEGER (10000..10512)

Z ::= BOOLEAN

-- Parameterization
AA {Param1, Param2} ::= SEQUENCE {
    a Param1,
    b Param2
}

AB ::= AA { R, Y }

AC ::= AA { Q, D }

-- All encoded values valid (1 bit).
AD ::= ENUMERATED {
    a,
    b
}

-- Extensibility - with additionals
AE ::= SEQUENCE {
    a BOOLEAN OPTIONAL,
    b BOOLEAN DEFAULT TRUE,
    c BOOLEAN,
    ...
}

AF ::= SEQUENCE {
    a BOOLEAN,
    ...,
    b AH,
    e INTEGER(0..255),
    f INTEGER(0..255),
    g INTEGER(0..255),
    h INTEGER(0..255),
    i INTEGER(0..255),
    j INTEGER(0..255) OPTIONAL,
    k INTEGER(0..255) DEFAULT 24,
    l INTEGER(0..255),
    ...
}

AG ::= SEQUENCE {
    a BOOLEAN,
    ...,
    b OCTET STRING (SIZE(0..10)),
    c SEQUENCE (SIZE (1..10)) OF BOOLEAN,
    d ENUMERATED {
        e, f(256), g(65536)
    },
    h NULL,
    i REAL (WITH COMPONENTS {
                     mantissa (-16777215..16777215),
                     base (2),
                     exponent (-149..104)
                 }),
    j CHOICE {
        k INTEGER (0..65535),
        l BOOLEAN
    },
    m OCTET STRING(SIZE(5)),
    ...
}

AH ::= SEQUENCE {
    c BOOLEAN,
    ...,
    d INTEGER(0..255),
    e ENUMERATED {f, g, h},
    ...
}

AI ::= SEQUENCE {
    a AK
}

AJ ::= SEQUENCE {
    a ENUMERATED {
        a-1, a-2
    }
}

AK ::= CHOICE {
    a AJ
}

AL ::= INTEGER (-129..127)

AM ::= INTEGER (-2..128)

AN ::= ENUMERATED { a(-16777216), b(-8388608), c(-65536), d(-32768), e(-128),
